---
apiVersion: k8s.crossplane.jonashackt.io/v1alpha1
kind: KubernetesCluster
metadata:
  namespace: default
  name: crossplane-eks
spec:
  id: crossplane-eks
  version: "1.30"
  parameters:
    region: eu-central-1
    nodes:
      count: 3
      tags:
       kubernetes.io/cluster/crossplane-eks: shared
       Name: crossplane-eks
    subnets:
      tags:
        kubernetes.io/cluster/crossplane-eks: owned
  writeConnectionSecretToRef:
    name: eks-cluster-kubeconfig